<template name="course-list-template">
  <block wx:for="{{item}}" wx:for-item="val">
    <view class='course-details'>
      <view>{{val}}</view>
      <text class='course-infor'>{{val.name}}</text>
      <view class='course-list'>
        <view class='lists'>
          <text class='course-charpet'>{{item.name}}</text>
          <navigator url='../study/study' class='naviagtors'>
            <view class='charpet-course-name'>
              <text class='iconfont icon-bofang'>{{item.name}}</text>
            </view>
          </navigator>
        </view>


      </view>
    </view>
  </block>
</template>